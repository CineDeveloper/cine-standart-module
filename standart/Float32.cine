rules
    1 = type == Float32
    join 1 BasicFloat

func abs(a Float32) a
    attributes
        alwaysInline
    include
        #include <math.h>
    C
        result_ = fabsf(a_);

func arcCos(a Float32) a
    attributes
        alwaysInline
    include
        #include <math.h>
    flag debug is set?
        then debugMessage(!a.inRange?(-1.0.convertTo(#a), 1.0.convertTo(#a)), "File: " builtIn_currentFile ", line: " builtIn_currentLine ". UB - wrong value.")
    C
        result_ = acosf(a_);

func arcCtg(a Float32) a
    attributes
        alwaysInline
    include
        #include <math.h>
    C
        result_ = (float)(M_PI) / 2.0 - atanf(a_);

func arcSin(a Float32) a
    attributes
        alwaysInline
    include
        #include <math.h>
    flag debug is set?
        then debugMessage(!a.inRange?(-1.0.convertTo(#a), 1.0.convertTo(#a)), "File: " builtIn_currentFile ", line: " builtIn_currentLine ". UB - wrong value.")
    C
        result_ = asinf(a_);

func arcTg(a Float32) a
    attributes
        alwaysInline
    include
        #include <math.h>
    C
        result_ = atanf(a_);

func ceil(a Float32) a
    attributes
        alwaysInline
    include
        #include <math.h>
    C
        result_ = ceilf(a_);

func cos(a Float32) a
    attributes
        alwaysInline
    include
        #include <math.h>
    C
        result_ = cosf(a_);

func ctg(a Float32) a
    attributes
        alwaysInline
    include
        #include <math.h>
    C
        result_ = (float)(1.0) / tanf(a_);

func disassemble(a Float32) Pair(a, a)
    attributes
        alwaysInline
    include
        #include <math.h>
    C
        result_.second_ = modff(a_, &(result_.first_));

func exp(a Float32) a
    attributes
        alwaysInline
    include
        #include <math.h>
    C
        result_ = expf(a_);

func floor(a Float32) a
    attributes
        alwaysInline
    include
        #include <math.h>
    C
        result_ = floorf(a_);

func fromString(a Float32, string [UInt8]) Box(a)
    attributes
        alwaysInline
    include
        #include <string.h>
    if string.getLength().inRange?(1, 312)
        then
            C
                char buf[string_.length + 1];
                memcpy(buf, string_.items, string_.length);
                buf[string_.length] = 0;
                char* endPtr;
                result_.private_item_ = strtof(buf, &endPtr);
                result_.private_haveItem_ = (uint64_t)(endPtr - buf) == string_.length;
                if (!result_.private_haveItem_) {
                    result_.private_item_ = 0.0;
                }

func lg(a Float32) a
    attributes
        alwaysInline
    include
        #include <math.h>
    C
        result_ = log10f(a_);

func ln(a Float32) a
    attributes
        alwaysInline
    include
        #include <math.h>
    C
        result_ = logf(a_);

func log(a Float32, base a) a
    attributes
        alwaysInline
    include
        #include <math.h>
    C
        result_ = logf(a_) / logf(base_);

func mod(a Float32, b a) a
    attributes
        alwaysInline
    include
        #include <math.h>
    C
        result_ = fmodf(a_, b_);
    flag debug is set?
        then debugMessage(result.nan?(), "File: " builtIn_currentFile ", line: " builtIn_currentLine ". UB - result is NaN.")

