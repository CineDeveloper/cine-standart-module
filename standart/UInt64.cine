rules
    1 = type == UInt64
    join 1 BasicUnsignedInteger

func countAllSet(a UInt64) UInt8
    C
        result_ = __builtin_popcountll(a_);

func findFirstSet(a UInt64) UInt8
    C
        result_ = __builtin_ffsll(a_);

func toSigned(a UInt64) Int64
    attributes
        alwaysInline
    flag debug is set?
        then debugMessage(a > (#a.maxValue() >> 1), "File: " builtIn_currentFile ", line: " builtIn_currentLine ". UB - signed number overflow.")
    C
        result_ = a_;
