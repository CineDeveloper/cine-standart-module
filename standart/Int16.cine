rules
    1 = type == Int16
    join 1 BasicSignedInteger

func toUnsigned(a Int16) UInt16
    attributes
        alwaysInline
    flag debug is set?
        then debugMessage(a < #a, "File: " builtIn_currentFile ", line: " builtIn_currentLine ". UB - assign a negative number to the unsigned variable.")
    C
        result_ = a_;
